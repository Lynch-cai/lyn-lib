{
    "name": "@coover-fr/lyn",
    "description": "Lyn is a Vue 3 UI components library",
    "private": false,
    "version": "0.0.2",
    "type": "module",
    "files": [
        "dist"
    ],
    "main": "./dist/lyn.umd.js",
    "module": "./dist/lyn.es.js",
    "exports": {
        ".": {
            "import": "./dist/lyn.es.js",
            "require": "./dist/lyn.umd.js"
        },
        "./dist/lyn.css": {
            "import": "./dist/lyn.css",
            "require": "./dist/lyn.css"
        }
    },
    "types": "./dist/main.d.ts",
    "scripts": {
        "dev": "vite",
        "build": "vue-tsc && vite build --config vite.lib.config.ts && storybook build",
        "preview": "vite preview",
        "storybook": "storybook dev -p 6006"
    },
    "dependencies": {
        "vue": "^3.2.45",
        "vue-router": "^4.2.5"
    },
    "devDependencies": {
        "@storybook/addon-essentials": "^7.5.2",
        "@storybook/addon-interactions": "^7.5.2",
        "@storybook/addon-links": "^7.5.2",
        "@storybook/blocks": "^7.5.2",
        "@storybook/testing-library": "^0.2.2",
        "@storybook/vue3": "^7.5.2",
        "@storybook/vue3-vite": "^7.5.2",
        "@typescript-eslint/eslint-plugin": "^6.9.0",
        "@typescript-eslint/parser": "^6.9.0",
        "@vitejs/plugin-vue": "^4.0.0",
        "eslint": "^8.52.0",
        "eslint-plugin-storybook": "^0.6.15",
        "eslint-plugin-vue": "^9.18.0",
        "path": "^0.12.7",
        "prettier": "^3.0.3",
        "rollup-plugin-typescript2": "^0.34.1",
        "storybook": "^7.5.3",
        "storybook-addon-pseudo-states": "^2.1.2",
        "stylus": "^0.59.0",
        "stylus-loader": "^7.1.3",
        "typescript": "^4.9.3",
        "vite": "^4.0.0",
        "vite-plugin-css-injected-by-js": "^3.3.0",
        "vite-plugin-dts": "^1.7.1",
        "vue-tsc": "^1.0.11"
    }
}
